<p id="notice"><%= notice %></p>

<h1>Listing Box Counts</h1>

<table>
  <thead>
    <tr>
      <th>Box count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @box_counts.each do |box_count| %>
      <tr>
        <td><%= box_count.box_count %></td>
        <td><%= link_to 'Show', box_count %></td>
        <td><%= link_to 'Edit', edit_box_count_path(box_count) %></td>
        <td><%= link_to 'Destroy', box_count, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Box count', new_box_count_path %>
